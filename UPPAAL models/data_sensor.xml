/*******************Variables*******************/

/**************Temporary Variables**************/
bool done;

/*******************Functions*******************/
void update(){

    if(m &lt; ((SENSORS-1)+BODYHUBS)){
        m++;
    } else {
        m = 0;
        done = true;
    }
}

void undo() {
    done = false;
}</declaration>
